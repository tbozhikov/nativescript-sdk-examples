<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" navigatedTo="onNavigatedTo">
	<Page.actionBar>
		<ActionBar title="{{ actionBarTitle }}" icon="" class="action-bar">
		</ActionBar>
	</Page.actionBar>
	<ListView items="{{ links }}" class="list-group" itemTap="{{ onItemTap }}">
		<ListView.itemTemplate>
			<GridLayout class="list-group-item">
				<Label text="{{ title }}" class="list-group-item-heading" />
			</GridLayout>
		</ListView.itemTemplate>
	</ListView>
</Page>
